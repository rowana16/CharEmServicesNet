@model CharEmServicesNet.Models.ViewModels.LocationPartialViewModel
<div >
    <table class="table table-hover">
        <tr>
            <th>Provider</th>
            <th>Service</th>            
            <th></th>
            <th></th>
        </tr>
        @foreach (var service in Model.services)
        {
            <tr>
                <td>@Html.DisplayFor(x => service.ServiceProviders.FirstOrDefault().OrganizationName)</td>
                <td>@Html.DisplayFor(x=> service.ServiceName) </td>                
                <td></td>
                <td>@Html.ActionLink("Get Info","ExtendedDetails","Service", new { id = service.Id },new { @class = "btn btn-primary" })</td>
            </tr>
        }

    </table>
</div>
